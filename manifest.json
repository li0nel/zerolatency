{
  "manifest_version": 3,
  "name": "Zero Latency Portal Emailer",
  "version": "1.0",
  "description": "Add the Portal Emailer to Chrome for an emailer shortcut",

  "icons": {
    "32": "images/icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://portal.zerolatencyvr.com/session/*"
      ]
    }
  ]
}
